{
    "rules": [
        {
            "name": "AWS Access Key ID",
            "regex": "AKIA[0-9A-Z]{16}",
            "description": "Access key for AWS pattern"
        },
        {
            "name": "AWS Secret Access Key",
            "regex": "(?<![A-Z0-9])[A-Za-z0-9/+=]{40}(?![A-Z0-9])",
            "description": "Secret Access key AWS pattern"
        },
        {
            "name": "Google Cloud API Key",
            "regex": "AIza[0-9A-Za-z-_]{35}",
            "description": " API key pattern for Google Cloud"
        },
        {
            "name": "Azure Storage Key, base64 encoded",
            "regex": "[A-Za-z0-9+/]{88}==",
            "description": "Base 64 encode pattern for Azure"
        },
        {
            "name": "DigitalOcean Personal Access Token",
            "regex": "dop_v1_[a-z0-9]{64}",
            "description": "PAT pattern for DigitalOcean"
        },
        {
            "name": "Slack Token",
            "regex": "xox[baprs]-[0-9A-Za-z-]{10,48}",
            "description": "Slack token pattern"
        },
        {
            "name": "Github classic token",
            "regex": "ghp_[A-Z-a-z0-9]{36}",
            "description": "Github classic token pattern"
        },
        {
            "name": "Github fine-grained personal access token",
            "regex": "github_pat_[A-Za-z0-9_]{82}",
            "description": "Pattern for github file-grained PAT"
        },
        {
            "name": "Stripe API Key",
            "regex": "sk_live_[0-9a-zA-Z]{24}",
            "description": "API Key pattern for Stripe"
        },
        {
            "name": "Twilio API key",
            "regex": "SK[0-9a-fA-F]{32}",
            "description": "Twilio API key pattern"
        },
        {
            "name": "Generic PEM Key",
            "regex": "-----BEGIN (RSA|DSA|EC|PGP|OPENSSH)? ?PRIVATE KEY-----",
            "description": "Generic Private Key Pattern"
        },
        {
            "name": "SSH Private Key",
            "regex": "-----BEGIN OPENSSH PRIVATE KEY-----",
            "description": "Private SSH Key Pattern"
        },
        {
            "name": "JWT",
            "regex": "eyJ[A-Za-z0-9_-]+\\.[A-Za-z0-9_-]+\\.[A-Za-z0-9_-]+",
            "description": "JSON Web Token pattern"
        },
        {
            "name": "Generic API Key",
            "regex": "[A-Za-z0-9_-]{20,}[=]{0,2}",
            "description": "Generic API Key pattern"
        },
        {
            "name": "Base64 encoded key",
            "regex": "(?<![A-Za-z0-9+/])[A-Za-z0-9+/]{40,}={0,2}(?![A-Za-z0-9+/])",
            "description": "Base64 encode pattern"
        }
    ]
}
